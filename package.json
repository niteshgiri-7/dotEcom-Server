    "eslint": "^9.20.1",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "nodemon": "^3.1.9",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.24.1"
