  "scripts": {
    "lint": "eslint src",
    "build": "rimraf dist && tsc",
    "start": "node dist/app.js",
    "ts.check": "tsc --project tsconfig.json",
    "add-build": "git add dist",
    "dev": "concurrently \"tsc -w\" \"nodemon dist/app.js\"",
