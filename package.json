    "express": "^4.21.2",
    "firebase-admin": "^13.1.0",
    "moment": "^2.30.1",
    "mongoose": "^8.9.3",
    "mongoose-to-swagger": "^1.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
